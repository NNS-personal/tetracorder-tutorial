	  common /key1/ ikeywd, keysiz, beg, lnend, ibeg, nend, set
	  common /key1/ cmdals, cmdtrn, numals, trnsiz, alsize

          character*16 ikeywd, cmdals(200)
	  character*60 cmdtrn(200)
   	  character*80 set

	  integer*4 keysiz, beg, lnend, numals, trnsiz(200)
	  integer*4 alsize(200), ibeg, nend
