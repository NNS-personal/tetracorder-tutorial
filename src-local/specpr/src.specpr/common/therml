      common /therml/   irodev,irofil,irsdev,irsfil,isodev,isofil,
                        ronorm,rsnorm,dist,potemp,pstemp,thrmltmp(5)
      integer*4 irodev,irofil,irsdev,irsfil,isodev,isofil
      real*4 ronorm,rsnorm,dist,potemp,pstemp,thrmltmp

# thermltmp is to flush out common block to 64 characters
